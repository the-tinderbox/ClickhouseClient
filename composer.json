{
  "name": "the-tinderbox/clickhouse-php-client",
  "description": "Clickhouse client over HTTP",
  "authors": [
    {
      "name": "FacedSID",
      "email": "ay@imagespark.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "Tinderbox\\Clickhouse\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tinderbox\\Clickhouse\\": "tests"
    }
  },
  "require": {
    "php": "~7.1",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "mockery/mockery": "^0.9.9",
    "phpunit/php-code-coverage": "^5.2",
    "phpunit/phpunit": "~4.0||~5.0||~6.0"
  },
  "scripts": {
    "test": "phpunit --coverage-text --colors=never"
  },
  "config": {
    "sort-packages": true
  }
}
